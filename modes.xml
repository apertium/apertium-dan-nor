<?xml version="1.0" encoding="UTF-8"?>
<modes>
  <mode name="da-nb" install="yes">
    <pipeline>
      <program name="lt-proc -e">
        <file name="da-nb.automorf.bin"/>
      </program>
      <program name="apertium-tagger -g $2">
        <file name="da-nb.prob"/>
      </program>
      <program name="apertium-pretransfer"/>
      <program name="lt-proc -b">
        <file name="da-nb.autobil.bin"/>
      </program>
      <program name="apertium-transfer -b">
        <file name="apertium-da-nb.da-nb.t1x"/>
        <file name="da-nb.t1x.bin"/>
      </program>
      <program name="lt-proc $1">
        <file name="da-nb.autogen.bin"/>
      </program>

    </pipeline>
  </mode>
  
  <mode name="da-nb-morph" install="no">
    <pipeline>
      <program name="lt-proc">
        <file name="da-nb.automorf.bin"/>
      </program>
    </pipeline>
  </mode>

  <mode name="da-nb-tagger" install="no">
